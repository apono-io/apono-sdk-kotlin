        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "active": {
            "type": "boolean"
          },
          "trigger_type": {
            "$ref": "#/components/schemas/TriggerType"
          },
          "grantees": {
