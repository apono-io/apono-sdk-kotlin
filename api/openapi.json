            }
          },
          "revoke_after_in_sec": {
            "format": "int32",
            "type": "integer"
          },
          "justification_required": {
            "type": "boolean"
          },
          "created_date": {
            "$ref": "#/components/schemas/Instant"
          },
          "timeframe": {
            "type": "object",
            "allOf": [
              {
                "$ref": "#/components/schemas/Timeframe"
              }
            ],
            "nullable": true
          }
        }
      },
      "AccessRequest": {
        "required": [
          "request_id",
          "friendly_request_id",
          "user_id",
          "status",
          "integration_id",
          "resource_ids",
          "permissions",
          "justification"
        ],
        "type": "object",
        "properties": {
          "request_id": {
            "type": "string"
          },
          "friendly_request_id": {
            "type": "string"
          },
          "user_id": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/AccessStatusModel"
          },
          "integration_id": {
            "type": "string"
          },
          "resource_ids": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "permissions": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "justification": {
            "type": "string"
          }
        }
      },
      "AccessStatusModel": {
        "enum": [
          "PENDING",
          "APPROVED",
          "REJECTED",
          "GRANTED",
          "REVOKING",
          "EXPIRED",
          "FAILED"
        ],
        "type": "string"
      },
      "AccessTargetModelV3": {
        "required": [
          "integration_id",
          "resource_type",
          "resource_tag_matchers",
          "permissions"
        ],
        "type": "object",
        "properties": {
          "integration_id": {
            "type": "string"
          },
          "resource_type": {
            "type": "string"
          },
          "resource_tag_matchers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TagModelV3"
            }
          },
          "permissions": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "ApproverModel": {
        "required": [
          "id",
          "type"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/ApproverTypeModel"
          }
        }
      },
